ocalhost:8000/expense/save\", {\r\n        title: expense.title,\r\n        amount: expense.amount,\r\n        date: expense.date\r\n      }, {\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: `Bearer ${token}`\r\n        }\r\n      });\r\n      // console.log(response);\r\n      const { expenseList } = response.data;\r\n      setUserExpenses(prevState => {\r\n        return (\r\n          expenseList.map(expense => {\r\n            //convert ISODate to Date. by default dates are stored as ISODates in mongo database\r\n            let date = new Date(expense.date);\r\n            date = new Date(date.getFullYear(), date.getMonth() + 1, date.getDate());\r\n            return {\r\n              id: expense._id,\r\n              title: expense.title,\r\n              amount: expense.amount,\r\n              date: date\r\n            };\r\n          })\r\n        );\r\n      });\r\n    };\r\n    getData();\r\n  };\r\n\r\n  const logoutHandler = () => {\r\n    navigate(\"/\");\r\n    setUserExpenses([]);\r\n    localStorage.clear();\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <div className={\"btn-container\"}>\r\n        <button onClick={logoutHandler}>Logout</button>\r\n      </div>\r\n      <NewExpense onAddExpense={addUserExpenseHandler} />\r\n      <Expenses items={userExpenses} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","D:\\Projects\\WebDev\\Proper Projects\\React Projects\\expenses\\client\\src\\components\\Signup.js",[],"D:\\Projects\\WebDev\\Proper Projects\\React Projects\\expenses\\client\\src\\components\\UI\\Backdrop.js",[],"D:\\Projects\\WebDev\\Proper Projects\\React Projects\\expenses\\client\\src\\components\\UI\\Overlay.js",[],"D:\\Projects\\WebDev\\Proper Projects\\React Projects\\Expenses\\client\\src\\index.js",[],"D:\\Projects\\WebDev\\Proper Projects\\React Projects\\Expenses\\client\\src\\App.js",[],"D:\\Projects\\WebDev\\Proper Projects\\React Projects\\Expenses\\clie